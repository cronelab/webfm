{
  "name": "webfm",
  "version": "3.0.0",
  "description": "A web-based functional mapping utility.",
  "main": "server/server.js",
  "scripts": {
    "start": "node --experimental-json-modules server/server.js",
    "dev": "nodemon --experimental-json-modules server/server.js",
    "docs:src_file": "typedoc --out docs/srcdoc src --mode file --name WebFM Documentation",
    "docs:src_modules": "typedoc --out docs/srcdoc src --mode modules --name WebFM Documentation",
    "docs:json": "typedoc --json docs/srcdoc.json --mode modules",
    "docs:server": "cd docs && make html"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "server/build/*",
      "migration/*",
      "data/*"
    ],
    "ext": "ts",
    "watch": [
      "src"
    ],
    "execMap": {
      "ts": "ts-node"
    }
  },
  "contributors": [
    {
      "name": "Christopher Coogan",
      "email": "c.coogan2201@gmail.com"
    },
    {
      "name": "Max Collard",
      "email": "collard@jhu.edu"
    }
  ],
  "type": "module",
  "license": "SEE LICENSE IN LICENSE.md",
  "sideEffects": false,
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@react-three/drei": "^2.2.3",
    "@types/jquery": "^3.5.4",
    "@types/mathjs": "^6.0.7",
    "@types/node": "^14.14.6",
    "@types/react-bootstrap": "^1.0.1",
    "babel-loader": "^8.1.0",
    "bci2k": "^2.3.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "css-loader": "^3.6.0",
    "csv-parse": "^4.14.0",
    "d3": "^5.16.0",
    "d3-axis": "^1.0.12",
    "d3-heatmap": "^1.2.1",
    "d3-horizon-chart": "0.0.6",
