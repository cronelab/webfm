{
  "exclude": [
    "./migration",
    "./server",
    "node_modules",
    "dist",
    "src/Components/*",
    "src/shared/",
  ],
  "compilerOptions": {
	"noImplicitAny": false,

	"outDir": "./dist2/",
    "sourceMap": true,
    "jsx": "react",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "incremental": true,
    "module": "es6",
    "allowSyntheticDefaultImports": true,
	"moduleResolution": "node",
	"strictNullChecks": false,
    "typeRoots": ["node_modules/@types"]
  },
}
